---
import "./styles.css";

import { SocialLinksData } from "../../data/social";

import SocialIconWithLink from "./SocialIconWithLink.astro";
---

<div class="social-icons-container">
	{
		SocialLinksData.map((socialLink) => (
			<SocialIconWithLink
				href={socialLink.url}
				icon={socialLink.iconName}
				title={socialLink.title}
			/>
		))
	}
</div>
